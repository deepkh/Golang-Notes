${GOGRPCFTPCLIENT_BIN}:
	$(CD) ${GOGRPCFTPCLIENT} && $(MAKE) -f Makefile.${GOGRPCFTPCLIENT_NAME} -j8 $@

GOGRPCFTPCLIENT_DEV: 
	$(CD) ${GOGRPCFTPCLIENT} && $(MAKE) -f Makefile.${GOGRPCFTPCLIENT_NAME} -j8 ${GOGRPCFTPCLIENT_BIN}
	@echo MAKE $@ 

